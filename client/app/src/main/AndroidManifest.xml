<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.share">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/appic"
        android:label="@string/app_name"
        android:roundIcon="@drawable/appic_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".HomeActivity" />
        <activity android:name=".ChattingActivity" />
        <activity android:name=".PayActivity" />
        <activity android:name=".ReservationInfoActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".ChatListActivity" />
        <activity android:name=".BucketListActivity" />
        <activity android:name=".ItemListActivity" />
        <activity android:name=".ItemDetailActivity"/>
        <activity android:name=".MapsMarkerActivity"/>
        <activity android:name=".AddCardDetailActivity"/>
        <activity android:name=".RegisterItemActivity"/>
        <activity android:name=".SelectDateActivity"
            android:theme="@style/Theme.MyDialog"/>
        <activity android:name=".MapsMarkerRegiActivity"/>
        <activity android:name=".ShareListActivity"/>
        <activity android:name=".BorrowListActivity"/>
        <activity android:name=".MyPageActivity"/>
        <activity
            android:name=".RegisterReviewActivity"
            android:theme="@style/Theme.MyDialog"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key"/>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
</manifest>